{
  "expo": {
    "name": "Voravia",
    "slug": "voravia",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "voravia",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EAF8F7"
    },
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "health.voravia.voravia",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Voravia uses your location to find nearby restaurants and rate menus based on your health profile.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Voravia uses your location to find nearby restaurants and rate menus based on your health profile.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "health.voravia.voravia",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EAF8F7"
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c58c743-23d6-4df6-8abd-522e801fa46e"
      }
    }
  }
}
